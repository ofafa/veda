<%- include header.ejs %>
<p><a href="/">Back</a></p>
<hr>
<p>搜尋結果：</p>
<% if (medicines.length == 0 && compositions.length == 0){ %>
<p>無相似結果</p>
<% } %>
<% medicines.forEach(function(medicine, index){ %>
<p><a href='/medicine/<%= medicine.name %>'><%= medicine.name %></a></p>
<p>位置：<%= medicine.position %></p>
<p>Col:<%= medicine.col %></p>
<p>Row:<%= medicine.row %></p>
<p>Index:<%= medicine.index %></p>
<hr>
<% }) %>

<% compositions.forEach((composition) => { %>
<p><a href="/composition/<%= composition.name %>"><%= composition.name %></a></p>
<p>type: <%= composition.type %></p>
<% }) %>

<hr>
<p><a href="/">Back</a></p>